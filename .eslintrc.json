{
  "env": {
    "browser": true,
    "jest/globals": true,
    "es6": true
  },
  "plugins": [
    "jest"
  ],
  "extends": [
    "eslint:recommended",
    "next/core-web-vitals",
    "plugin:jest/recommended",
    "plugin:jest/style"
  ],
  "rules": {
    "jest/no-alias-methods": "off",
    "no-unused-vars": ["error", {
      "varsIgnorePattern": "^_",
      "argsIgnorePattern": "^_"
    }]
  }
}